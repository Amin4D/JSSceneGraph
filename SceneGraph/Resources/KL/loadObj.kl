operator loadObj(
  io FabricResource resource,
  io Data objParseHandle
) {
  FabricOBJDecode(resource.data, resource.dataSize, objParseHandle);
  Size nbPoints;
  FabricOBJGetNbPoints(objParseHandle, nbPoints);
  report "OBJ nbPoints: " + nbPoints;
}
