operator loadUniform(
  io OGLShaderProgram shaderProgram,
  io DATA_TYPE value
) {
  //  report("loadATTRIBUTE_NAMEUniform");
  Integer location = getUniformLocation(ATTRIBUTE_ID);
  if(location!=-1){
    shaderProgram.loadDATA_TYPEUniform( location, value );
  }
}

